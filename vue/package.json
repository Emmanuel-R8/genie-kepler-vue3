    "Web Map"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/geospatialweb/go-vue3-tsx"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview",
