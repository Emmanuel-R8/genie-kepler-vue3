    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "sass": "^1.35.1",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.19.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.4",
    "vite": "^2.3.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
